<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="aiofwa.org.a3dsnooker">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/eight"
        android:label="@string/app_name"
        android:roundIcon="@drawable/eight"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SlideyActivity"
            android:label="@string/title_activity_slidey">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".NotificationBlabber"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".SnookerActivity" />
        <activity android:name=".MainActivity">
<!--            <intent-filter>-->
<!--                <action android:name="android.intent.action.MAIN" />-->

<!--                <category android:name="android.intent.category.LAUNCHER" />-->
<!--            </intent-filter>-->
        </activity>
    </application>

</manifest>
